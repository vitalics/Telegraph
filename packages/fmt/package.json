{
  "name": "@tlgr/fmt",
  "version": "1.5.2",
  "description": "Formatter functions for telegram reply context",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "Telegram",
    "utility",
    "formatting",
    "JSX"
  ],
  "author": "Vitali Haradkou",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/vitalics/Telegraph/issues"
  },
  "homepage": "https://github.com/vitalics/Telegraph/tree/main/packages/fmt#readme",
  "dependencies": {
    "@mdx-js/preact": "2.1.3",
    "awesome-phonenumber": "3.3.0",
    "node-html-parser": "6.1.0",
    "preact": "10.11.0",
    "preact-render-to-string": "5.2.4",
    "react-dom": "^17.0.2",
    "telegraf": "4.8.6"
  },
  "devDependencies": {
    "tsup": "7.2.0"
  }
}